# Production Environment Variables for Render
PORT=10000

# Google OAuth Configuration (Production) - SET THESE IN RENDER DASHBOARD
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://rscans.netlify.app/auth/callback

# JWT Secret for token signing - GENERATE A SECURE SECRET
JWT_SECRET=your_secure_jwt_secret_here

# Production Database (if using MongoDB)
MONGODB_URI=mongodb://localhost:27017/resume-builder

# Environment
NODE_ENV=production
